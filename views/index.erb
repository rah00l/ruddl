
<div id="itemList">
<div id="itemListToolbar">
  <div id="links">
    <a href="http://news.ycombinator.com" id="top" class="active">Top</a>
    <a href="http://news.ycombinator.com/newest" id="newest">New</a>
        <span class="right">
          <a id="refresh" href="#refresh" title="Refresh">
            <img src="data:image/png;base64," alt="Refresh" height="20" width="20">
          </a>
        </span>
  </div>
</div>

<div id="items" class="scrollbar" style="height: 1045px; ">
  <% @docs.each do |doc| %>
    <div class="item">
      <h2><a href="<%= doc.key %>"><%= doc.title %></a></h2>
      <div class="detail">
        <div class="right">
          <span class="site-link"><a href="http://www.spacex.com/press.php?page=20121008" target="_blank" title="Go to page" class="go_to_site"><img src="data:image/png;base64," alt="Go to Site" height="15" width="15"></a></span>
          <span class="comments"><a href="http://news.ycombinator.com/item?id=4628591" title="View Comments"><img src="data:image/png;base64," height="15" width="18" alt="View Comments"></a></span>
        </div>
        <span class="site">www.spacex.com</span>
      </div>
    </div>
  <% end %>
</div>
</div>

<div id="itemDetail">
  <% @docs.each do |doc| %>
      <div id="<%= doc.key %>" class="rightside">
        <%= doc.content.encode("UTF-8") %>
      </div>
  <% end %>
</div>
